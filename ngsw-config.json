{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "The Formatted Blog",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://stackpath.bootstrapcdn.com/font-awesome/**",
          "https://fonts.googleapis.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "drafts-api",
      "urls": ["https://theformattedblog-snakone.b4a.run/drafts"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 12,
        "maxAge": "4h",
        "timeout": "5s"
      }
    }
  ]
}

